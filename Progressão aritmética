public class MyClass {
    public static void main(String args[]) {
  
  int numero1 = 1;
  int numero2 = 2;
  int numero3 = 3;
  int numero4 = 4;
  int numero5 = 28;
  int numero6 = 29;
  int numero7 = 30;
  
  System.out.println(numero1 + numero2 + numero3 + numero4 + numero5 + numero6 + numero7);
  
  
  
    }
}
